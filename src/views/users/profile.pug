extends ../base
include ../mixins/video


block content
    div.profile__data
        if isheroku
            img.avatar(src=loggedInUser.avatarUrl)
        else 
            img.avatar(src="/"+loggedInUser.avatarUrl)
        h3=user.name
    div.video-grid
        each video in user.videos
            +video(video)
        else
            span.empty__message No videos found